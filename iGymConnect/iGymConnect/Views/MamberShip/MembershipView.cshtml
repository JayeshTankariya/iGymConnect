
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MembershipView</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>

<body>
    <div class="page-header "text-center">
        <h1><center>Welcome to iGymConnect</center></h1>
    </div>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
          
            <button type="button" class="btn btn-group btn-primary" data-toggle="modal" data-target="#membership" style = "width:32%">
              Member
            </button>
            <!-- Modal -->
            <div class="modal fade" id="membership" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Member Information</h4>
                        </div>
                        <div class="modal-body">
                            <form class="form-horizontal">
                                <div class="form-group">
                                    <label for="id" class="col-sm-3 control-label">Member Id:</label>
                                    <div class="col-sm-8">
                                        <input class="form-control" id="id" >
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="name" class="col-sm-3 control-label">Name:</label>
                                    <div class="col-xs-4">
                                        <input class="form-control" id="name">
                                    </div>
                                    <div class="col-xs-5">
                                        <input class="form-control" id="name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Gender" class="col-sm-3 control-label">Gender:</label>
                                    <div>
                                        <label for="male" class="col-xs-2 control-label">
                                            Male
                                            <input type="radio">
                                        </label>
                                        <label for="male" class="col-sm-3 control-label">
                                            Female
                                            <input type="radio" >
                                        </label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="address" class="col-sm-3 control-label">Address:</label>
                                    <div class="col-sm-8">
                                        <input class="form-control" id="address">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="line2" class="col-sm-3 control-label">Line 2:</label>
                                    <div class="col-sm-8">
                                        <input class="form-control" id="line2">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="area" class="col-sm-3 control-label">City,State & Zip:</label>
                                    <div class="col-xs-2">
                                        <input class="form-control" id="city">
                                    </div>
                                    <div class="col-xs-3">
                                        <input class="form-control" id="State">
                                    </div>
                                    <div class="col-xs-4">
                                        <input class="form-control" id="Zip">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="phone1" class="col-sm-3 control-label">Phone #1:</label>
                                    <div class="col-xs-2">
                                        <input class="form-control" id="HOME">
                                    </div>
                                    <div class="col-xs-3">
                                        <input class="form-control" id="HOME" placeholder="HOME">
                                    </div>
                                    
                                </div>
                                <div class="form-group">
                                    <label for="phone2" class="col-sm-3 control-label">Phone #2:</label>
                                    <div class="col-xs-2">
                                        <input class="form-control" id="WORK">
                                    </div>
                                    <div class="col-xs-3">
                                        <input class="form-control" id="WORK" placeholder="WORK">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="email" class="col-sm-3 control-label">Email:</label>
                                    <div class="col-sm-8">
                                        <input type="email"class="form-control" id="email">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="note" class="col-sm-3 control-label">Note:</label>
                                    <div class="col-xs-3">
                                        <textarea class="form-control" rows="3"></textarea>
                                    </div>
                                </div>

                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary">Save changes</button>
                        </div>
                    </div>
                </div>
            </div>
           

            <button type="button" class="btn btn-group btn-primary" data-toggle="modal" data-target="#employee"style = "width:33%">
                Membership Type
            </button>

            <!-- Modal -->
            <div class="modal fade" id="employee" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Membership Type</h4>
                        </div>
                        <div class="modal-body">
                            <form class="form-horizontal">
                                <div class="form-group">
                                    <label for="id" class="col-sm-4 control-label">Membership Type Id:</label>
                                    <div class="col-sm-4">
                                        <input class="form-control" id="id">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="id" class="col-sm-4 control-label">Description:</label>
                                    <div class="col-sm-6">
                                        <input class="form-control" id="id">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="date" class="col-sm-4 control-label">Active Date:</label>
                                    <div class="col-sm-6">
                                        <input type="date"class="form-control" id="date">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="date" class="col-sm-4 control-label">InActive Date:</label>
                                    <div class="col-sm-6">
                                        <input type="date" class="form-control" id="date">
                                    </div>
                                </div>
                               &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                    <label>
                                        <input type="checkbox" value="">
                                        Guest Membership
                                    </label>
                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                <label>
                                    <input type="checkbox" value="">
                                    Staff Membership
                                </label>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-primary">Add</button>
                            <button type="button" class="btn btn-primary">Update</button>
                            <button type="button" class="btn btn-primary">Delete</button>
                            <button type="button" class="btn btn-primary">Cancle</button>
                            <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>


            <button type="button" class="btn btn-group btn-primary" data-toggle="modal" data-target="#myModal"style = "width:33%">
                Employee
            </button>

            <!-- Modal -->
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Employee</h4>
                        </div>
                        <div class="modal-body">
                            <form class="form-horizontal">
                                <div class="form-group">
                                    <label for="id" class="col-sm-4 control-label">Employee Id:</label>
                                    <div class="col-sm-4">
                                        <input class="form-control" id="id">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="id" class="col-sm-4 control-label">AdharCard Id:</label>
                                    <div class="col-sm-4">
                                        <input class="form-control" id="id">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="id" class="col-sm-4 control-label">Member Id:</label>
                                    <div class="col-sm-4">
                                        <input class="form-control" id="id">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="name" class="col-sm-4 control-label">Full Name:</label>
                                    <div class="col-sm-7">
                                        <input class="form-control" id="name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="add" class="col-sm-4 control-label">Address:</label>
                                    <div class="col-sm-7">
                                        <input class="form-control" id="add">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="city" class="col-sm-4 control-label">City:</label>
                                    <div class="col-sm-5">
                                        <input class="form-control" id="city">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="state" class="col-sm-4 control-label">State:</label>
                                    <div class="col-sm-5">
                                        <input class="form-control" id="state">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="zip" class="col-sm-4 control-label">Zip:</label>
                                    <div class="col-sm-5">
                                        <input class="form-control" id="zip">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Position" class="col-sm-4 control-label">Position:</label>
                                    <div class="dropdown">
                                        <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                            Select Position
                                        </button>
                                        <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                            <li><a href="#">Personal Trainer</a></li>
                                            <li><a href="#">DailyCare</a></li>
                                            <li><a href="#">Sales</a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="date" class="col-sm-4 control-label">Hire Date:</label>
                                    <div class="col-sm-6">
                                        <input type="date" class="form-control" id="date">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="note" class="col-sm-4 control-label">Note:</label>
                                    <div class="col-xs-3">
                                        <textarea class="form-control" rows="3"></textarea>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="modal-footer">
                            
                            <button type="button" class="btn btn-primary">Add</button>
                            <button type="button" class="btn btn-primary">Update</button>
                            <button type="button" class="btn btn-primary">Delete</button>
                            <button type="button" class="btn btn-primary">Cancle</button>
                            <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>


</div>

</nav>
</body>
</html>
<script src="@Url.Content("~/scripts/jquery-1.10.2.min.js")"></script>

<script src="@Url.Content("~/scripts/bootstrap.min.js")"></script>
