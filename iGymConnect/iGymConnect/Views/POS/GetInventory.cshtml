<script>
    var invId = 0;
    var invs;
    var CId = 0;
    var cid;
    var TransactionMasterId = 0;
    var CardItemsId = 0;
    var now = new Date();

    var a = $('#btnCash').select();


    function Final() {
        var GrandTotal = 0;
        $('#tblCart tbody tr').each(function () {
            var value = parseFloat($(this).find('td:last').text());
            GrandTotal += value;
        });
        $('#txtGrandTotal').val(GrandTotal);
    }
    var paymentMode = 'CASH';
    var paymentMode1 = 'GIFT CARD';
    $(document).ready(function () {
        $('#txtBarcode').focus();

        $('body').on('click', '#tblcat tbody tr', function () {
            $('#txtQty').val("");
            invId = $(this).data('invid');
            invs = $(this);
            $('#myModal').modal('show');
        });

        $('#btnPaymentByCash').on('click', function () {
            paymentMode = 'CASH';
            $('#myCash').modal('show');
            $.ajax({
                type: "POST",
                url: '@Url.Action("CashPayment", "POS")',
                data: { Id: invId },
                success: function (data) {
                    if (data != null) {
                        $('#myCash').each(function (index) {
                            if ($('#tblCart tbody tr td:last') != null) {
                                $('#txtItemTotal').val($('#txtGrandTotal').val());
                            }
                        });
                    }
                }
            });
        });
        $('#btnPaymentByGiftCard').on('click', function () {
            $("#lblEnterMember").show();
            $('#lblEnterMember').focus();
            $('#lblShowTotal').val($('#txtGrandTotal').val());
        });
        
        $('#lblEnterMember').on('blur', function () {
            if ($('#lblEnterMember').val() == "") {
                $('#myGF').modal('hide');
                alert("Plz. Enter Member To Continue Transaction");
            }
            else {
                $('#myGF').modal('show');
                $.ajax({
                    type: "GET",
                    url: '@Url.Action("GetCardNoByMember", "POS")',
                    data: { Id: $('#lblEnterMember').val() },
                    success: function (data) {
                        if (data != null) {
                            $('#ddlCardNumber').empty();
                            $.each(data, function (index, element) {
                                $('#ddlCardNumber').append('<option value = "' + element.Id + '">' + element.Number + '</option>');
                                //$("#ddlCardNumber").append($('<option></option>').attr("value", data.Id).text(data.Number));
                            });
                        }
                    }
                });
            }
            
        });

        $('#btnAddCard').on('click', function () {
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetCardDetails", "POS")',
                data: { Id: $('#ddlCardNumber').val() },
                success: function (data) {
                    if (data != null) {
                        var date = new Date(parseInt(data.ExpiryDate.substr(6)));
                        var datestr = date.getDate() + "/" + (date.getMonth() + 1) + "/" + date.getFullYear();

                        $('#tblCard tbody').append('<tr data-cid="' + data.Id +
                          '"><td>' + data.Number +
                          '</td><td>' + data.Amount +
                          '</td><td>' + datestr +
                          '</td></tr>');
                        //$.each(data, function (index, element) {
                        //    $('#ddlCardNumber option['+element.Id == element.Number+']').remove();
                        //});
                    }
                }
            });
        });


