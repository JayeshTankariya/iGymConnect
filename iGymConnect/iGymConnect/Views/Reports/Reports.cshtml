        <script>
            $(document).ready(function () {
                $('#POS').on('click', function () {
                    $('#POSModal').modal('show');
                });

                $('#btnSelect').on('click', function () {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("AllocateCard", "Reports")',
                        data: { Id: $('#ddlMemberList1').val() },
                        success: function (data) {
                            if (data != null) {
                                window.location = "/iGymConnect/Content/PDF/" + data;

                            }
                        }
                    });
                });

                $('#btnSelectInvByCat').on('click', function () {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("InvByCat", "Reports")',
                        data: { Id: $('#ddlCategoryList1').val() },
                        success: function (data) {
                            if (data != null) {
                                window.location = "/iGymConnect/Content/PDF/" + data;

                            }
                        }
                    });
                });

                $('#btnGo').on('click', function () {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("DateWiseTransaction", "Reports")',
                        data: { dt: $('#dtTransactionDate').val() },
                        success: function (data) {
                            if (data != null) {
                                window.location = "/iGymConnect/Content/PDF/" + data;

                            }
                        }
                    });
                });

                $('#btnShow').on('click', function () {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("TranRange", "Reports")',
                        data: { dt: $('#dt').val(), dt1: $('#dt1').val() },
                        success: function (data) {
                            if (data != null) {
                                window.location = "/iGymConnect/Content/PDF/" + data;

                            }
                        }
                    });
                });

                $('#btnReportCategory').on('click', function () {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("GetCategory", "Reports")',
                        data: { },
                        success: function (data) {
                            if (data != null) {
                                window.location = "/iGymConnect/Content/PDF/" + data;

                            }
                        }
                    });
                });

                $('#btnReportInventory').on('click', function () {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("GetInventory", "Reports")',
                        data: {},
                        success: function (data) {
                            if (data != null) {
                                window.location = "/iGymConnect/Content/PDF/" + data;

                            }
                        }
                    });
                });

                $('#btnReportCardGenerated').on('click', function () {
                    $.ajax({
